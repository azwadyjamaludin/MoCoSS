<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/indCounTL2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TableRow
        android:id="@+id/contentRowInd2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleRowInd">

        <TextView
            android:id="@+id/numDataInd2"
            android:layout_width="38dp"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dateEditInd2"
            android:layout_width="118dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sesCodeEditInd2"
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sesHourEditInd2"
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/sesRptBtnInd2"
            android:layout_width="151dp"
            android:layout_height="35dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/abstract_geo"
            android:textAllCaps="false" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/avBtnInd2"
            android:layout_width="151dp"
            android:layout_height="35dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/abstract_geo"
            android:textAllCaps="false" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <EditText
            android:id="@+id/notesInd2"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine" />
    </TableRow>

</TableLayout>