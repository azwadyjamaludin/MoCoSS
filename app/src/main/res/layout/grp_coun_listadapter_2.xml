<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/grpCounTL2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TableRow
        android:id="@+id/contentRowGrp2"
        android:layout_width="1000dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleRowInd">

        <TextView
            android:id="@+id/numDataGrp2"
            android:layout_width="38dp"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dateEditGrp2"
            android:layout_width="118dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sesCodeEditGrp2"
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/sesHourEditGrp2"
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:textStyle="bold" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/sesRptBtnGrp2"
            android:layout_width="151dp"
            android:layout_height="wrap_content" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/avBtnGrp2"
            android:layout_width="151dp"
            android:layout_height="wrap_content" />

        <Space
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <EditText
            android:id="@+id/notesGrp2"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine" />
    </TableRow>
</TableLayout>